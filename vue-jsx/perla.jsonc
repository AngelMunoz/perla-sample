{
  "$schema": "https://raw.githubusercontent.com/AngelMunoz/Perla/v0.20.1/perla.schema.json",
  "index": "./index.html",
  "devServer": {
    "mountDirectories": {
      "./src": "/src"
    },
    "liveReload": true
  },
  "build": {
    "jsxFactory": "h",
    "jsxFragment": "Fragment",
    "injects": [
      "./jsx-shim.js"
    ]
  },
  "packages": {
    "vue": "https://cdn.skypack.dev/pin/vue@v3.2.22-ADhoiepkqZzWxHvjJF6y/mode=imports,min/optimized/vue.js"
  }
}